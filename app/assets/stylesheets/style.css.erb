
body{

	
	margin:0px;
	padding:0px;
	background-color: #eeeeee
	
}


/*this is the dashboard styles below */
.dashboard_wrapper{

	margin: auto;
	background-color: white;
	width:80%;
	padding:30px;
	padding-bottom:70px;
	box-shadow: 0px 3px 5px black
}



.dashboard_header{
	font-family: 'Source Sans Pro' , serif;
	margin:20px auto;
	width:80%;
	text-align:center;
	background-color:#eeeeee;
	border-radius: 30%;
	padding:2%;
}


#logo div{
	
	float:right;
	border:1px solid white;
	padding:10px;
	border-radius:60%;
	background-color: white
	
}

#logo span img{
	border-radius: 60%;
	border:1px solid #eeeeee;
}
	

.invoicer_div{
	margin:10px;
	font-size:18px;
	font-family:arial;
}

.Menu{
	width:40%;
	margin:auto;
	text-align:center;
	background-color:white;
}


.Menu ul li{
	list-style-type: none;
	text-decoration:none;
	margin-bottom: 30px
}

.Menu ul li a{
	text-decoration:none;
	display:block;
	height:100px;
	background-color:#eeeeee;
	border-radius: 100%;
	padding-top: 50px;
	color:black;
	font-size:17px;
	font-family: arial;
	box-shadow: 0px 3px 5px black

}

.Menu ul li a:hover{
	text-decoration:none;
	display:block;
	height:100px;
	background-color:#eeeeee;
	border-radius: 100%;
	padding-top: 50px;
	color:black;
	font-size:17px;
	font-family: arial;
	box-shadow: 0px 3px 5px grey

}


.profile_table{
	width:80%;
	background-color:silver;
	margin:auto;
	padding:10px;
	box-shadow: 0px 3px 5px grey
}

.profile_table tr td{
padding:19px;
text-align: center;
font-family:sans-serif,arial;
font-size:14px;
letter-spacing: 0.5px;
background-color:#eeeeee;
border-radius: 10px 0px 0px 10px


}


.profile_table tr td input{
width:80%;
text-align: center;
padding:7px;
box-shadow: 0px 2px 2px silver

}

.profile_table tr td select{
width:80%;
text-align: center;
padding:7px;
box-shadow: 0px 2px 2px silver

}



.profile_table tr{


padding:10px;

}

.profile_table th{


padding:10px;
font-size:20px !important;
text-align: left;
color:#eeeeee;
font-weight:bold;
letter-spacing:0.3px;
font-family:tahoma;

}

.profile_table th span{


float:right;
color:darkblue;
font-family:tahoma;

}

#profile_error_div{

	border:1px solid silver;
	position:absolute;
	background-color:#fffeee;
	margin-left:200px;
	width:70%;

	box-shadow: 0px 5px 5px #eeeeee;


}




.tddata{
	font-family:tahoma !important;
	font-size:18px;
	background-color:#eeeeee !important;
	padding:0px !important;
	border-radius: 0px 10px 10px 0px !important;
	font-weight:2em;
	width:30%;
	padding:10px !important;
	
}

.tddata1{
	font-family:tahoma !important;
	font-size:18px;
	background-color:#eeeeee !important;
	padding:0px !important;
	border-radius: 0px 10px 10px 0px !important;
	font-weight:2em;
	width:30%;
	padding:10px !important;
	text-align:center !important;
	
}

.tddata1 div{
	background-color:white !important;
	text-align:center !important;
	border-radius: 100%;
	margin:auto;
	
}


.tddata1 div img{
	width:60%;
	
}






.welcome{

	width:960px;
	margin:auto;
	background-color: white;
	padding:15px;
	height:400px;
	color:grey;
	text-decoration:underline;
}

.welcome_links{
	width:60%;
	margin:auto;
	text-align: center;

}

.welcome_links li{
	display:inline-block;
	margin-right:20px;
	text-align:center;
	width:30%;
	background-color:#eeeeee;
	margin:5px;
	height:50px;
	padding-top:30px;
	border-radius:100%;
	box-shadow: 0px 3px 5px black
	

}

.welcome_links li:hover{
	background-color:silver;
	color:#eeeeee;

}

.welcome_links li a:hover{
	background-color:silver;
	color:#eeeeee;

}


.welcome_links li a{
color:grey;
text-decoration: none;
font-family:sans-serif;
letter-spacing:1px;
	
}



h1{

	text-align: center;
	
}
.table_wrapper{
	background-color: white;
	width: 960px;
	margin: auto;
	border-radius: 5px;
	box-shadow: 0px 3px 5px rgba(0,0,0,0.4)

}

.table_wrapper table {

border:0px solid grey;

}

.table_wrapper table td {

padding :10px;
font-family: 'Source Sans Pro' , serif;
font-size:14px;
letter-spacing:0.5px;
width:100%;

}

.table_wrapper table td input {

box-shadow: 0px 3px 5px rgba(0,0,0,0.4)

}


.table_wrapper table td select {

width:100%

}

#sign_up_error_div{

	border:1px solid silver;
	position:absolute;
	background-color:#fffeee;
	margin:0px;
	width:960px;

	box-shadow: 0px 5px 5px #eeeeee;


}



.inner_table_div{
	margin: auto;
	width: 430px;
	margin-top: 10px;
	padding-top:10px;
	
}



.table_wrapper fieldset{
	border: 0px solid yellow;
	
}

.legend_text{
	background-color: #eeeeee;
	border-radius: 7px;
	font-size:20px;
	padding:10px;
	font-family: 'Source Sans Pro' , serif;
}


.field_with_errors {

    border:1px solid red;
    

}

.errorli{
color:red;
font-family: 'Source Sans Pro' , serif;
font-size:12px;

}

.errorpara{
	color:red;
	
	font-weight:bold;
	font-family:tahoma;
	font-size:17px;
}

.thank_you_div{
width:60%;
margin:100px auto;
text-align:center;
font-family:tahoma;
font-size:15px;
background-color:white;
height:300px;
padding-top:100px;
box-shadow: 0px 3px 5px white

}


.thank_you_div div{
width:60%;
margin:0px auto;
font-size:20px;
background-color:#eeeeee;
padding:40px;
box-shadow: 0px 5px 5px silver

}

.thank_you_div span{
cursor:pointer;
text-decoration:underline;
color:blue;



}


.loginwrapper{
		width:60%;
		margin:40px auto;
		background-color:white;
		box-shadow: 0px 3px 5px grey

}

.loginwrapper table{
		width:60%;
		margin:auto;
		padding:10px;
		font-family: 'Source Sans Pro' , serif;
		font-size:14px;
		

}

.loginwrapper table input{
	
	box-shadow: 0px 3px 5px #eeeeee

}

.loginwrapper table td {
	
	padding:10px;

}


#loginflash{
	color:red;
	text-decoration:none;
	font-family: 'Source Sans Pro' , serif;
}


.client_wrapper{

width:90%;
margin:10px auto;
background-color:white;
padding:10px;
box-shadow: 0px 3px 5px black


}

.client_wrapper table{

width:90%;
margin:auto;


}

.client_wrapper table tr td{
	
padding:10px;
text-align:center;
font-family:arial;
font-size:18px;
box-shadow: 0px 3px 5px #eeeeee;
text-transform: capitalize;

}


.client_wrapper table tr td a{
	
font-size:12px;
display: block;
margin-bottom: 2px

}



.client_wrapper table tr th{
	
text-align:center;
font-family:tahoma;
font-size:18px;
border:1px solid #eeeeee;
padding:15px;
width:400px;

}

.client_wrapper table tr th a{
	
font-size:14px !important;
font-weight: bold

}

.client_add_div{

	padding:10px;
	text-align:center;

}

.pagination a{
padding:5px;
border:1px solid darkblue;
text-decoration: none;
font-size:30px;
}

.client_add_div a:link{
	color:blue;
}

.client_add_div span{
	margin-right:15px;
}